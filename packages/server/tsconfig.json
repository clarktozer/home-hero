{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "lib": ["es5", "es6"],
        "target": "es5",
        "module": "CommonJS",
        "rootDir": "./src",
        "outDir": "./dist",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "strictPropertyInitialization": false
    }
}
